{
	"name": "datovelger",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://frodehansen2.github.io/datovelger",
	"dependencies": {
		"@types/react": "^16.0.40",
		"chokidar": "^2.0.2",
		"nav-frontend-chevron": "^1.0.2",
		"nav-frontend-core": "^4.0.5",
		"nav-frontend-typografi": "^2.0.2",
		"react": "^16.2.0",
		"react-dates": "^16.3.6",
		"react-dom": "^16.2.0",
		"react-scripts-ts": "2.13.0"
	},
	"scripts": {
		"start-js": "react-scripts-ts start",
		"build-js": "react-scripts-ts build",
		"start": "npm-run-all -p build-js build-css start-js",
		"start-dev": "npm-run-all -p build-css watch-css start-js",
		"build": "npm-run-all build-css build-js",
		"test": "react-scripts-ts test --env=jsdom",
		"eject": "react-scripts-ts eject",
		"build-css": "node-less-chokidar src/styles/index.less -o src/",
		"watch-css": "chokidar \"src/**/*.less\" -c \"node-less-chokidar ./src/styles/index.less\"",
		"watch-css-old": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build"
	},
	"resolutions": {
		"@types/react": "^16.0.40"
	},
	"devDependencies": {
		"@types/jest": "^22.2.0",
		"@types/node": "^9.4.6",
		"@types/react-dates": "^16.0.5",
		"@types/react-dom": "^15.5.7",
		"classnames": "^2.2.5",
		"gh-pages": "^1.1.0",
		"nav-frontend-chevron-style": "^0.3.4",
		"nav-frontend-knapper-style": "^0.3.6",
		"nav-frontend-skjema-style": "^0.3.14",
		"nav-frontend-typografi-style": "^1.0.7",
		"node-less-chokidar": "^0.1.2",
		"npm-run-all": "^4.1.2",
		"tslint-config-prettier": "^1.9.0",
		"typescript": "^2.7.2"
	}
}
